<h1>Pages#home</h1>
<p>Go to <%= link_to "Store", spree_path %></p>

<% if spree_current_user %>
	<p>Logged in as <%= spree_current_user.email %></p>
	<%= link_to "Logout", spree.logout_path, data { turbo_method: "delete" } %>
<% else %>
	<p>Not Logged in</p>
	<%= link_to "Login", spree.login_path %>
	<%= link_to "Create Account", spree.signup_path %>
<% end %>